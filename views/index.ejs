<script type="text/javascript">

    function readURL(input, thumbimage) {
        if (input.files && input.files[0]) { //Sử dụng  cho Firefox - chrome
            var reader = new FileReader();
            reader.onload = function (e) {
                $("#thumbimage").attr('src', e.target.result);
            }
            reader.readAsDataURL(input.files[0]);
        }
        else { // Sử dụng cho IE
            $("#thumbimage").attr('src', input.value);

        }
        $("#thumbimage").show();
        $('.filename').text($("#uploadfile").val());
        $('.Choicefile').css('background', '#C4C4C4');
        $('.Choicefile').css('cursor', 'default');
        $(".removeimg").show();
        $(".Choicefile").unbind('click'); //Xóa sự kiện  click trên nút .Choicefile

    }
    $(document).ready(function () {
        var html = '';
        $('#edit').click(function () {
            $('#edit').hide();
            $('#ok').show();
            $('#exit').show();
            html = $('#myform').html();
            var company = $('#myform>h5').text();
            var name = $('#myform>h4').text().slice(6);
            var title = $('#myform>p').text().slice(7);
            var phone = $('#myform span').first().text().slice(7);
            var email = $('#myform span').last().text().slice(7);
            var html_after = '';
            html_after += "<table cellpadding='10'>";


            html_after += "<tr>";
            html_after += "<td>";
            html_after = html_after + "Company:";
            html_after += "</td>";
            html_after += "<td>";
            html_after = html_after + "<input class='form-control' type='text' name='company' value='" + company + "'/>";
            html_after += "</td>";
            html_after += "</tr>";

            html_after += "<tr>";
            html_after += "<td>";
            html_after = html_after + "Name:";
            html_after += "</td>";
            html_after += "<td>";
            html_after = html_after + "<input class='form-control' type='text' name='name' value='" + name + "'/>";
            html_after += "</td>";
            html_after += "</tr>";

            html_after += "<tr>";
            html_after += "<td>";
            html_after = html_after + "Title:";
            html_after += "</td>";
            html_after += "<td>";
            html_after = html_after + "<input class='form-control' type='text' name='title' value='" + title + "'/>";
            html_after += "</td>";
            html_after += "</tr>";

            html_after += "<tr>";
            html_after += "<td>";
            html_after = html_after + "Phone:";
            html_after += "</td>";
            html_after += "<td>";
            html_after = html_after + "<input class='form-control' type='text' name='phone' value='" + phone + "'/>";
            html_after += "</td>";
            html_after += "</tr>";
            html_after += "<tr>";
            html_after += "<td>";
            html_after = html_after + "Email:";
            html_after += "</td>";
            html_after += "<td>";
            html_after = html_after + "<input class='form-control' type='text' name='email' value='" + email + "'/>";
            html_after += "</td>";
            html_after += "</tr>";

            html_after += "</table>";
            console.log(html_after);
            $('#myform').html(html_after);
        });
        $('#exit').click(function () {
            $('#ok').hide();
            $('#exit').hide();
            $('#myform').html(html);
            $('#edit').show();
        });

        $('#demo').click(function () {
            console.log($('#left1').css('height'));
        });
        $(".Choicefile").bind('click', function () { //Chọn file khi .Choicefile Click
            $("#uploadfile").click();

        });
        $(".removeimg").click(function () {//Xóa hình  ảnh đang xem
            $("#thumbimage").attr('src', '').hide();
            $("#myfileupload").html('<input type="file" id="uploadfile"  onchange="readURL(this);" />');
            $(".removeimg").hide();
            $(".Choicefile").bind('click', function () {//Tạo lại sự kiện click để chọn file
                $("#uploadfile").click();
            });
            $('.Choicefile').css('background', '#0877D8');
            $('.Choicefile').css('cursor', 'pointer');
            $(".filename").text("");
        });

        $("div.bhoechie-tab-menu>div.list-group>a").click(function (e) {
            e.preventDefault();
            $(this).siblings('a.active').removeClass("active");
            $(this).addClass("active");
            var index = $(this).index();
            $("div.bhoechie-tab>div.bhoechie-tab-content").removeClass("active");
            $("div.bhoechie-tab>div.bhoechie-tab-content").eq(index).addClass("active");
        });


    });
</script>


